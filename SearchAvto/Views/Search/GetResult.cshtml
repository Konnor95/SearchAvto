@model CarModel
@{
    ViewBag.Title = "Test";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <h2>Результат</h2>
    <div class="test" id="test">
        <div id="subdiv" style="display: block;height: 95%;">
            <div class="pad-15">
                <p>Вам подходит @Model.FullName</p>
                <div class="col-md-4">@HtmlManager.Image(Model.Image, Model.FullName, "max-height: 400px;margin: 0 auto; display: block;")</div>
            </div>
            <div style="text-align: center;">
                @Html.ActionLink("Перейти к модели", "Model", "Search", new { id = Model.Id }, new { @class = "btn btn-1" })
                @Html.ActionLink("Пройти тест еще раз", "TestPage", "Search", null, new { @class = "btn btn-1" })
            </div>
        </div>

        <div style="position: relative;bottom: 5px;margin: 10px 5px" id="bar-container" class="progress progress-striped">
            <div id="bar" class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">Тест завершен</div>
        </div>
    </div>
</div>